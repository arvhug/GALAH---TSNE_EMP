@article{nordlander_lowest_2019,
	title = {The lowest detected stellar {Fe} abundance: the halo star {SMSS} {J160540}.18-144323.1},
	volume = {488},
	issn = {0035-8711},
	shorttitle = {The lowest detected stellar {Fe} abundance},
	url = {http://adsabs.harvard.edu/abs/2019MNRAS.488L.109N},
	doi = {10.1093/mnrasl/slz109},
	abstract = {We report the discovery of SMSS J160540.18-144323.1, a new ultra 
metal-poor halo star discovered with the SkyMapper telescope. We measure
[\{Fe\}/\{H\}]= -6.2 ± 0.2 (1D LTE), the lowest ever detected
abundance of iron in a star. The star is strongly carbon-enhanced,
[\{C\}/\{Fe\}] = 3.9 ± 0.2, while other abundances are compatible
with an α-enhanced solar-like pattern with [\{Ca\}/\{Fe\}] = 0.4
± 0.2, [\{Mg\}/\{Fe\}] = 0.6 ± 0.2, [\{Ti\}/\{Fe\}] = 0.8 ±
0.2, and no significant s- or r-process enrichment, [\{Sr\}/\{Fe\}] {\textless} 0.2
and [\{Ba\}/\{Fe\}] {\textless} 1.0 (3σ limits). Population III stars
exploding as fallback supernovae may explain both the strong carbon
enhancement and the apparent lack of enhancement of odd-Z and
neutron-capture element abundances. Grids of supernova models computed
for metal-free progenitor stars yield good matches for stars of about 10
M\_⊙ imparting a low kinetic energy on the supernova ejecta, while
models for stars more massive than roughly 20 M\_⊙ are incompatible
with the observed abundance pattern.},
	urldate = {2021-01-19},
	journal = {Monthly Notices of the Royal Astronomical Society},
	author = {Nordlander, T. and Bessell, M. S. and Da Costa, G. S. and Mackey, A. D. and Asplund, M. and Casey, A. R. and Chiti, A. and Ezzeddine, R. and Frebel, A. and Lind, K. and Marino, A. F. and Murphy, S. J. and Norris, J. E. and Schmidt, B. P. and Yong, D.},
	month = sep,
	year = {2019},
	keywords = {stars: abundances, stars: individual: SMSS J160540.18-144323.1, stars: Population III},
	pages = {L109--L113},
	file = {Full Text PDF:/Users/arvdog/Zotero/storage/DB4M9ZH6/Nordlander et al. - 2019 - The lowest detected stellar Fe abundance the halo.pdf:application/pdf},
}

