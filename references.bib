@article{gustafsson_grid_2008,
	title = {A grid of {MARCS} model atmospheres for late-type stars - {I}. {Methods} and general properties},
	volume = {486},
	copyright = {© ESO, 2008},
	issn = {0004-6361, 1432-0746},
	url = {https://www.aanda.org/articles/aa/abs/2008/30/aa09724-08/aa09724-08.html},
	doi = {10.1051/0004-6361:200809724},
	abstract = {{\textless}i{\textgreater}Context. {\textless}i/{\textgreater}In analyses of stellar spectra and colours, and for the analysis of integrated light from galaxies, a homogeneous grid of model atmospheres of late-type stars and corresponding flux spectra is needed.{\textless}i{\textgreater}Aims. {\textless}i/{\textgreater}We construct an extensive grid of spherically-symmetric models (supplemented with plane-parallel ones for the highest surface gravities), built on up-to-date atomic and molecular data, and make it available for public use.{\textless}i{\textgreater}Methods. {\textless}i/{\textgreater}The most recent version of the MARCS program is used.{\textless}i{\textgreater}Results. {\textless}i/{\textgreater}We present a grid of about 10{\textless}sup{\textgreater}4{\textless}sup/{\textgreater} model atmospheres for stars with 2500 K {\textless}i{\textgreater}≤{\textless}i/{\textgreater} {\textless}i{\textgreater}T{\textless}i/{\textgreater}{\textless}sub{\textgreater}eff{\textless}sub/{\textgreater} {\textless}i{\textgreater}≤{\textless}i/{\textgreater} 8000 K, -1 {\textless}i{\textgreater}≤{\textless}i/{\textgreater} log {\textless}i{\textgreater}g{\textless}i/{\textgreater} = log {\textless}i{\textgreater}≤{\textless}i/{\textgreater} 5 (cgs) with various masses and radii, -5 {\textless}i{\textgreater}≤{\textless}i/{\textgreater} [Me/H] {\textless}i{\textgreater}≤{\textless}i/{\textgreater} +1, with [ {\textless}i{\textgreater}α{\textless}i/{\textgreater}/Fe] = 0.0 and 0.4 and different choices of C and N abundances. This includes “CN-cycled” models with C/N = 4.07 (solar), 1.5 and 0.5, C/O ranging from 0.09 to (normally) 5.0 to also represent stars of spectral types R, S and N, and with 1.0 {\textless}i{\textgreater}≤{\textless}i/{\textgreater} {\textless}i{\textgreater}≤{\textless}i/{\textgreater} 5 km s{\textless}sup{\textgreater}-1{\textless}sup/{\textgreater}. We also list thermodynamic quantities ({\textless}i{\textgreater}T{\textless}i/{\textgreater}, {\textless}i{\textgreater}P{\textless}i/{\textgreater}{\textless}sub{\textgreater}g{\textless}sub/{\textgreater}, {\textless}i{\textgreater}P{\textless}i/{\textgreater}{\textless}sub{\textgreater}e{\textless}sub/{\textgreater}, {\textless}i{\textgreater}ρ {\textless}i/{\textgreater}, partial pressures of molecules, etc.) and provide them on the World Wide Web, as well as calculated fluxes in approximately 108 000 wavelength points. Underlying assumptions in addition to 1D stratification (spherical or plane-parallel) include hydrostatic equilibrium, mixing-length convection and local thermodynamic equilibrium. We discuss a number of general properties of the models, in particular in relation to the effects of changing abundances, of blanketing, and of sphericity. We illustrate positive and negative feedbacks between sphericity and molecular blanketing. We compare the models with those of other available grids and find excellent agreement with plane-parallel models of Castelli \& Kurucz (if convection is treated consistently) within the overlapping parameter range. Although there are considerable departures from the spherically-symmetric NextGen models, the agreement with more recent PHOENIX models is gratifying.{\textless}i{\textgreater}Conclusions. {\textless}i/{\textgreater}The models of the grid show considerable regularities, but some interesting departures from general patterns occur for the coolest models due to the molecular opacities. We have tested a number of approximate “rules of thumb” concerning effects of blanketing and sphericity and often found them to be astonishingly accurate. Some interesting new phenomena have been discovered and explored, such as the intricate coupling between blanketing and sphericity, and the strong effects of carbon enhancement on metal-poor models. We give further details of line absorption data for molecules, as well as details of models and comparisons with observations in subsequent papers.},
	language = {en},
	number = {3},
	urldate = {2021-01-19},
	journal = {Astronomy \& Astrophysics},
	author = {Gustafsson, B. and Edvardsson, B. and Eriksson, K. and J{\o}rgensen, U. G. and Nordlund, {\AA} and Plez, B.},
	month = aug,
	year = {2008},
	note = {Number: 3
Publisher: EDP Sciences},
	pages = {951--970},
	file = {Snapshot:/Users/arvdog/Zotero/storage/E2ZCGXIZ/aa09724-08.html:text/html;Full Text PDF:/Users/arvdog/Zotero/storage/BY3QLYME/Gustafsson et al. - 2008 - A grid of MARCS model atmospheres for late-type st.pdf:application/pdf},
}

